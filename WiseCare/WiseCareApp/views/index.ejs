	  /* ---------- Map ---------- */
	$(function(){
	  $('#map').vectorMap({
	    map: 'us_mill',
	    color: 'red',
	    hoverOpacity: 0.7,
	    series: {
	      regions: [{
	        values: gdpData,
	        scale: ['#000', '#000'],
	        normalizeFunction: 'polynomial'
	      }]
	    },
		backgroundColor: '#AABBDD'
	  });
	});
	
